<% @accounts.each do |account| %>
  <pre>
    available_cash = <%= account.available_cash %>
    accrued_interest = <%= account.accrued_interest %>
    outstanding_principal = <%= account.outstanding_principal %>
    account_total = <%= account.account_total %>
    total_notes = <%= account.total_notes %>
    total_portfolios = <%= account.total_portfolios %>
    in_funding_balance = <%= account.in_funding_balance %>
    received_interest = <%= account.received_interest %>
    received_principal = <%= account.received_principal %>
    received_late_fees <%= account.received_late_fees %>
  </pre>
<% end %>
